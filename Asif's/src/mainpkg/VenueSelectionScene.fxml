<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #E3AE79;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.VenueSelectionSceneController">
   <children>
      <ComboBox fx:id="venueComboBox" layoutX="150.0" layoutY="75.0" prefHeight="34.0" prefWidth="150.0" promptText="Select" />
      <Label layoutX="31.0" layoutY="71.0" prefHeight="42.0" prefWidth="95.0" text=" Venue Name">
         <font>
            <Font name="Consolas Bold" size="13.0" />
         </font></Label>
      <Label layoutX="326.0" layoutY="76.0" prefHeight="25.0" prefWidth="212.0" text="Goverment Permission Granted">
         <font>
            <Font name="Calibri Bold" size="15.0" />
         </font></Label>
      <RadioButton fx:id="yesRadioButton" layoutX="326.0" layoutY="119.0" mnemonicParsing="false" text="Yes" />
      <RadioButton fx:id="noRadioButton" layoutX="327.0" layoutY="156.0" mnemonicParsing="false" text="No" />
      <Button layoutX="186.0" layoutY="152.0" mnemonicParsing="false" onAction="#saveButtonOnClick" prefHeight="25.0" prefWidth="57.0" text="Save" />
      <Button layoutX="186.0" layoutY="200.0" mnemonicParsing="false" onAction="#showButtonOnclick" prefHeight="25.0" prefWidth="57.0" text="Show" />
      <Button layoutX="499.0" layoutY="337.0" mnemonicParsing="false" onAction="#backButtonOnClick" text="Back" />
   </children>
</AnchorPane>
