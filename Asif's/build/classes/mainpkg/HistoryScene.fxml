<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #b3f5f0;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.HistorySceneController">
   <children>
      <ComboBox fx:id="historybookComboBox" layoutX="195.0" layoutY="70.0" prefWidth="150.0" promptText="Select" />
      <Label layoutX="71.0" layoutY="74.0" prefHeight="17.0" prefWidth="105.0" text="History Book List">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TableView fx:id="historyTable" layoutX="160.0" layoutY="163.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="bookColumn" prefWidth="199.0" text="Book Name" />
          <TableColumn minWidth="0.0" prefWidth="0.0" text="C2" />
        </columns>
      </TableView>
      <Button layoutX="234.0" layoutY="115.0" mnemonicParsing="false" onAction="#saveButtonOnClick" prefHeight="25.0" prefWidth="57.0" text="Save" />
      <Button layoutX="370.0" layoutY="163.0" mnemonicParsing="false" onAction="#showButtonOnClick" prefHeight="25.0" prefWidth="57.0" text="Show" />
      <Button layoutX="499.0" layoutY="338.0" mnemonicParsing="false" onAction="#backbuttonOnClick" prefHeight="25.0" prefWidth="57.0" text="Back" />
   </children>
</AnchorPane>
