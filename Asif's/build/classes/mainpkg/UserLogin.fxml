<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ede9b7;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.UserLoginController">
   <children>
      <Label layoutX="100.0" layoutY="93.0" prefHeight="17.0" prefWidth="30.0" text="ID">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Label>
      <Label layoutX="82.0" layoutY="136.0" text="Password">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Label>
      <TextField fx:id="idTextField" layoutX="166.0" layoutY="89.0" onMouseClicked="#ideditOneClick" promptText="Inter your id...." />
      <PasswordField fx:id="passwordTextField" layoutX="166.0" layoutY="134.0" onMouseClicked="#passwordeditOneClick" promptText="Inter your password..." />
      <Button layoutX="156.0" layoutY="220.0" mnemonicParsing="false" onAction="#loginButtonOnClick" prefHeight="35.0" prefWidth="169.0" text="Log In" />
      <Button layoutX="156.0" layoutY="275.0" mnemonicParsing="false" onAction="#newaccountOnClick" prefHeight="35.0" prefWidth="169.0" text="Create New Account" />
      <Button layoutX="204.0" layoutY="167.0" mnemonicParsing="false" onAction="#forgotButtonOnClick" prefHeight="17.0" prefWidth="111.0" text="Forgot Password" textFill="#eb3131" />
      <Label layoutX="152.0" layoutY="35.0" prefHeight="28.0" prefWidth="290.0" text="Book Fair Management System">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <ComboBox fx:id="userCB" layoutX="334.0" layoutY="89.0" prefWidth="150.0" promptText="Select User" />
   </children>
</AnchorPane>
